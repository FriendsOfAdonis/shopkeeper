{
  "name": "@foadonis/shopkeeper",
  "description": "",
  "version": "0.0.1-0",
  "engines": {
    "node": ">=20.6.0"
  },
  "type": "module",
  "files": [
    "build/src",
    "build/providers",
    "build/services",
    "build/stubs",
    "build/index.d.ts",
    "build/index.js"
  ],
  "exports": {
    ".": "./build/index.js",
    "./types": "./build/src/types.js",
    "./providers/*": "./build/providers/*.js",
    "./services/*": "./build/services/*.js",
    "./commands": "./build/commands/main.js",
    "./errors": "./build/src/errors/main.js"
  },
  "scripts": {
    "clean": "del-cli build",
    "copy:templates": "copyfiles \"stubs/**/*.stub\" build",
    "copy:resources": "copyfiles \"resources/**/*.edge\" build",
    "index:commands": "adonis-kit index build/commands",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "format": "prettier --write .",
    "quick:test": "node --import=./tsnode.esm.js --enable-source-maps bin/test.ts",
    "test": "c8 npm run quick:test",
    "prebuild": "npm run lint && npm run clean",
    "build": "tsc",
    "dev": "tsc --watch",
    "postbuild": "pnpm run copy:templates && pnpm run index:commands",
    "release": "np",
    "version": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/assembler": "^7.7.0",
    "@adonisjs/core": "^6.12.0",
    "@adonisjs/eslint-config": "^2.0.0-beta.6",
    "@adonisjs/lucid": "^21.2.0",
    "@adonisjs/prettier-config": "^1.3.0",
    "@adonisjs/tsconfig": "^1.3.0",
    "@japa/api-client": "^2.0.3",
    "@japa/assert": "^3.0.0",
    "@japa/expect": "^3.0.2",
    "@japa/file-system": "^2.3.0",
    "@japa/plugin-adonisjs": "^3.0.1",
    "@japa/runner": "^3.1.4",
    "@swc/core": "^1.6.3",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.14.5",
    "luxon": "^3.5.0",
    "c8": "^10.1.2",
    "copyfiles": "^2.4.1",
    "del-cli": "^5.1.0",
    "eslint": "^9.9.0",
    "html-entities": "^2.5.2",
    "np": "^10.0.6",
    "prettier": "^3.3.2",
    "reflect-metadata": "^0.2.2",
    "sqlite3": "^5.1.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "@adonisjs/core": "^6.2.0",
    "@adonisjs/lucid": "^21.2.0",
    "luxon": "^3.5.0",
    "reflect-metadata": "^0.2.2"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "np": {
    "message": "chore(release): %s",
    "tag": "latest",
    "branch": "main",
    "anyBranch": false
  },
  "c8": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "prettier": "@adonisjs/prettier-config",
  "dependencies": {
    "stripe": "^16.12.0"
  }
}
